<div id="WorkflowSetup" class="modal hide fade modal-xwide ui-draggable" tabindex="-1" role="dialog" aria-labelledby="AssessmentDefineTemplateLabel" aria-hidden="true">
  <div class="modal-inner-content">
    {{#instance}}
      <section class="content">
        <div class="hideable-holder">
          <div class="basic-info">
            <div class="info">

              {{> /static/mustache/mockup_base_templates/object_setup/fields_visibility_control.mustache}}
              <a class="modal-dismiss" href="javascript://" data-dismiss="modal">
                <i class="fa fa-times black"></i>
              </a>

              <div class="choose-object-wrap">
                <div class="row-fluid">
                  <div class="span8">
                    <div class="row-fluid wrap-row">
                      <div class="span12">
                        <h6>Workflow title</h6>
                        {{#w_title}}
                          {{> /static/mustache/mockup_base_templates/object_setup/object_input.mustache}}
                        {{/w_title}}
                      </div>
                    </div>
                    <div class="row-fluid wrap-row">
                      <div class="span12 hidable" data-value="workflow_description">
                        <h6>Description</h6>
                        {{#w_desc}}
                          {{> /static/mustache/mockup_base_templates/object_setup/object_textarea.mustache}}
                        {{/w_desc}}
                      </div>
                    </div>
                    <div class="row-fluid">
                      <div class="span12 hidable hidden" data-value="workflow_custom">
                        <h6>Custom "Start Workflow" email message</h6>
                        {{#w_custom}}
                          {{> /static/mustache/mockup_base_templates/object_setup/object_textarea.mustache}}
                        {{/w_custom}}
                      </div>
                    </div>
                  </div>
                  <div class="span4">
                    <div class="row-fluid wrap-row">
                      <div class="span12">
                        <h6>People</h6>
                        {{> /static/mustache/mockup_base_templates/info_panel/people_list.mustache}}
                      </div>
                    </div>
                    <div class="row-fluid wrap-row">
                      <div class="span12">
                        <h6>Frequency</h6>
                        <div class="side-content">
                          {{#frequency}}
                            {{> /static/mustache/mockup_base_templates/object_setup/object_select.mustache}}
                          {{/frequency}}
                        </div>
                      </div>
                    </div>
                    <div class="row-fluid">
                      <div class="span6">
                        <h6>Code</h6>
                        <div class="side-content">
                          {{#w_code}}
                            {{> /static/mustache/mockup_base_templates/object_setup/object_input.mustache}}
                          {{/w_code}}
                        </div>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>

          {{#task}}
            <div class="relevant-filter-group">
              {{> /static/mustache/mockup_base_templates/object_setup/repeating_block.mustache}}
            </div>
          {{/task}}
        </div>

        <div class="info">
          <div class="save-template-wrap">
            <div class="pull-right">
              {{> /static/mustache/mockup_base_templates/object_setup/template_master_controls.mustache}}
            </div>
          </div>
        </div>
      </section>
    {{/instance}}
  </div>
</div>
