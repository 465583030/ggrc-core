{{^if hasErrors}}
  {{#unless instance._disabled}}
    {{#if hasVerifiers}}
        <button class="btn btn-small btn-primary pull-right btn-info-pin-header"
                data-name="status"
                data-value="Ready for Review">
            Complete
        </button>
    {{else}}
        <button class="btn btn-small btn-primary pull-right btn-info-pin-header"
                data-name="status"
                data-value="Completed">Complete
        </button>
    {{/if}}
  {{/unless}}
{{else}}
    <button class="btn btn-small btn-primary pull-right btn-info-pin-header disabled" {{#if instance._mandatory_msg }}title="{{instance._mandatory_msg}}"{{/if}}>Complete</button>
{{/if}}

