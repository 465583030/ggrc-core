<div class="w-status">
    <span class="entry-author">
     <person-list-item person="modified_by"></person-list-item>
      {{assignee_types assignee_type}} added a response - {{localize_date created_at}}
    </span>
  {{#if hasRevision}}
      <h6 class="comment-list__ca-description">{{customAttributeData}}</h6>
  {{/if}}
    <div class="rtf-block">
     <read-more text="itemText"></read-more>
    </div>
</div>
