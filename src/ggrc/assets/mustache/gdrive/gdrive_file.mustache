<button
  class="{{link_class}} btn btn-small btn-white"
  {{#isInactive}}disabled{{/isInactive}}
  {{#if tooltip}}
    rel="tooltip"
    data-placement="bottom"
    data-original-title="{{tooltip}}"
  {{/if}}
  ($click)="onClickHandler"
  ($keyup)="onKeyup(%element, %event)">
  <content></content>
</button>
