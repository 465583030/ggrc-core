{{#if instance.viewLink}}
  {{^openIsHidden}}
  {{^instance.snapshot}}
  <a href="{{instance.viewLink}}" target="_blank"
     class="btn btn-mini btn-outline"
     can-click="openObject">
    Open
  </a>
  {{/instance.snapshot}}
  {{/openIsHidden}}
  {{^viewIsHidden}}
  <a href="#" class="btn btn-mini btn-outline" can-click="maximizeObject">
    View
  </a>
  {{/viewIsHidden}}
{{/if}}
